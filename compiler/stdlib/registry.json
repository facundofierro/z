{
  "version": "0.1.0",
  "targets": {
    "next": {
      "description": "React-based web applications with Next.js",
      "mode": "markup",
      "allowedChildren": [
        "Routes",
        "API",
        "Components",
        "Schema"
      ],
      "defaultPackages": {
        "next": "^14.0.0",
        "react": "^18.0.0",
        "@types/react": "^18.0.0",
        "typescript": "^5.0.0",
        "tailwindcss": "^3.0.0"
      },
      "compiler": "@z-compiler/nextjs"
    },
    "swift": {
      "description": "iOS/macOS applications with SwiftUI",
      "mode": "markup",
      "allowedChildren": [
        "App",
        "Components"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/swiftui"
    },
    "rust": {
      "description": "Native Rust applications or WebAssembly modules",
      "mode": "code",
      "allowedChildren": [
        "type",
        "fun",
        "mod"
      ],
      "defaultPackages": {
        "serde": "1.0",
        "wasm-bindgen": "0.2"
      },
      "compiler": "@z-compiler/rust"
    },
    "tauri": {
      "description": "Cross-platform desktop apps with web frontend",
      "mode": "markup",
      "allowedChildren": [
        "Frontend",
        "Backend",
        "Config"
      ],
      "defaultPackages": {
        "@tauri-apps/api": "^1.0.0",
        "@tauri-apps/cli": "^1.0.0"
      },
      "compiler": "@z-compiler/tauri"
    },
    "android": {
      "description": "Android applications with Kotlin/Java",
      "mode": "markup",
      "allowedChildren": [
        "Activities",
        "Services",
        "Components"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/android"
    },
    "harmony": {
      "description": "HarmonyOS applications",
      "mode": "markup",
      "allowedChildren": [
        "Pages",
        "Components"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/harmony"
    },
    "qt": {
      "description": "Cross-platform Qt applications",
      "mode": "markup",
      "allowedChildren": [
        "Windows",
        "Components"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/qt"
    },
    "java": {
      "description": "Java applications",
      "mode": "code",
      "allowedChildren": [
        "class",
        "interface",
        "package"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/java"
    },
    "python": {
      "description": "Python applications",
      "mode": "code",
      "allowedChildren": [
        "class",
        "function",
        "module"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/python"
    },
    "bash": {
      "description": "Bash shell scripts",
      "mode": "code",
      "allowedChildren": [
        "function",
        "script"
      ],
      "defaultPackages": {},
      "compiler": "@z-compiler/bash"
    }
  },
  "namespaces": {
    "Routes": {
      "aliasOf": "namespace",
      "description": "Web application routing structure",
      "allowedChildren": ["*"]
    },
    "API": {
      "aliasOf": "namespace",
      "description": "Backend API endpoints",
      "allowedChildren": ["*"]
    },
    "Components": {
      "aliasOf": "namespace",
      "description": "Reusable UI components",
      "allowedChildren": ["component"]
    },
    "Schema": {
      "aliasOf": "element",
      "description": "Database schema definitions",
      "allowedChildren": [
        "model",
        "type"
      ]
    },
    "App": {
      "aliasOf": "component",
      "role": "root",
      "description": "Application entry point"
    }
  },
  "annotations": {
    "@params": {
      "description": "Function parameter types",
      "usage": "functions"
    },
    "@response": {
      "description": "Function return type",
      "usage": "functions"
    },
    "@doc": {
      "description": "Human-readable documentation",
      "usage": "all"
    },
    "@context": {
      "description": "AI context information",
      "usage": "all"
    }
  }
}
